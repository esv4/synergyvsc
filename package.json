{
    "name": "synergex-vscode-synergydbl",
    "displayName": "Synergy DBL",
    "description": "Simple implementation of Synergy DBL in VS Code",
    "version": "0.1.0",
    "publisher": "Synergex",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "repository": "unpublished",
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "Synergy DBL",
                "configuration": "./language-configuration.json",
                "extensions": [
                    ".dbl",
                    ".dbc"
                ]
            }
        ],
        "grammars": [
            {
                "language": "Synergy DBL",
                "scopeName": "source.dbl",
                "path": "./syntaxes/Synergex.SynergyDBL.tmLanguage.json"
            }
        ],
        "commands": [],
        "snippets": [
            {
                "language": "Synergy DBL",
                "path": "./snippets/snippets.json"
            }
        ],
        "menus": {
            "explorer/context": []
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.10.7",
        "mocha": "^6.2.1",
        "tslint": "^5.20.0",
        "typescript": "^2.9.2",
        "vscode": "^1.1.36"
    },
    "dependencies": {
        "@types/vscode": "^1.38.0"
    }
}
